{
    "name": "baskooijmaninc/suzie",
    "type": "symfony",
    "description": "Kooijman Incorporate Holding B.V. Suzie is an Orm and FormBuilder Bundle based on symfony and bootstrap",
    "keywords": [
        "Suzie Orm FormBuilder"
    ],
    "license": "MIT",
    "minimum-stability": "stable",
    "prefer-stable": true,
    "authors": [
        {
            "name": "Sebastiaan Kooijman",
            "email": "kooijmaninc@gmail.com",
            "role": "Owner / Creator / Main Developer"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-mysqli": "*",
        "symfony/intl": "~7.0",
        "symfony/config": "~7.0",
        "symfony/console": "~7.0"
    },
    "require-dev": {
        "symfony/stopwatch": "~7.0",
        "psr/log": "*",
        "psr/container": "*"
    },
    "autoload": {
        "psr-4": {
            "KooijmanInc\\Suzie\\": "src/Suzie/",
            "KooijmanInc\\Suzie\\FormBuilder\\": "src/FormBuilder/",
            "KooijmanInc\\Suzie\\Model\\": "src/Model/",
            "KooijmanInc\\Suzie\\Exception\\": "src/Exception/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
